{
  "public": false,
  "github": {
    "enabled": false,
    "silent": true
  },
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "Strict-Transport-Security",
          "value" : "max-age=7776000;"
        },
        {
          "key" : "X-Content-Type-Options",
          "value" : "nosniff"
        },
        {
          "key" : "X-XSS-Protection",
          "value" : "1; mode=block"
        },
        {
          "key" : "X-Frame-Options",
          "value" : "SAMEORIGIN"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/static/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=600"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "application/json"
        },
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=60"
        }
      ]
    },
    {
      "source": "/feed.xml",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "application/rss+xml"
        },
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/atom.xml",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "application/atom+xml"
        },
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/feed.json",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "application/feed+json"
        },
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=3600"
        }
      ]
    },
    {
      "source": "/jsonp/(.*)",
      "headers" : [
        {
          "key" : "Content-Type",
          "value" : "text/javascript"
        },
        {
          "key" : "X-Content-Type-Options",
          "value" : ""
        },
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "prose.io" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version" }
      ]
    },
    {
      "source": "/.well-known/nostr.json",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    },
    {
      "source": "/keys/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/pgp-keys"
        }
      ]
    },
    {
      "source": "/.well-known/openpgpkey/hu/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Content-Type",
          "value": "application/octet-stream"
        }
      ]
    }
  ],
  "redirects": [
    { "source": "/pgp", "destination": "/pgp.txt", "permanent": false },
    { "source": "/utils", "destination": "/code", "permanent": false },
    { "source": "/utilities", "destination": "/code", "permanent": false },
    { "source": "/fileshute", "destination": "http://web.archive.org/web/20220112113123/https://fileshute.com/", "permanent": false },
    { "source": "/twitter", "destination": "https://twitter.com/pirafrank", "permanent": false },
    { "source": "/mastodon", "destination": "https://mastodon.social/@pirafrank", "permanent": false },
    { "source": "/threads", "destination": "https://threads.net/@pirafrank", "permanent": false },
    { "source": "/linkedin", "destination": "https://linkedin.com/in/pirafrank", "permanent": false },
    { "source": "/github", "destination": "https://github.com/pirafrank", "permanent": false },
    { "source": "/gist", "destination": "https://gist.github.com/pirafrank?direction=desc&sort=updated", "permanent": false },
    { "source": "/keybase", "destination": "https://keybase.io/pirafrank", "permanent": false },
    { "source": "/keyoxide", "destination": "https://keyoxide.org/00F17C2C8570B0822DF02CC6DC805C4BD79EC530", "permanent": false },
    { "source": "/aspe", "destination": "https://keyoxide.org/aspe%3Akeyoxide.org%3AQITV3373KPVHKJ5YESI6AW5EJQ", "permanent": false },
    { "source": "/telegram", "destination": "https://t.me/pirafrank", "permanent": false },
    { "source": "/meet", "destination": "https://cal.com/pirafrank/meet", "permanent": false },
    { "source": "/certificates", "destination": "/certs", "permanent": false }
  ]
}
